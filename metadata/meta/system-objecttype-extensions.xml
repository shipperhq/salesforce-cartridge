        <custom-attribute-definitions>
            <attribute-definition attribute-id="shipperHQRates">
                <display-name xml:lang="x-default">Use ShipperHQ Rates</display-name>
                <description xml:lang="x-default">Enabling this functionality associate this method with rates returned from ShipperHQ, only showing the method when an applicable rate is provided. When disabled, the method falls back to native functionality.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
